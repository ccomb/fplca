        <activityName xml:lang="en">water treatment (volume unit test)</activityName>
        <generalComment>
          <text xml:lang="en" index="0">Tests volume unit conversions: m³ (output) ↔ l (input) ↔ ml (chemicals)</text>
        </generalComment>
      </activity>
      <geography geographyId="34dbbff8-88ce-11de-ad60-0019e336be3a">
        <shortname xml:lang="en">GLO</shortname>
      </geography>
      <timePeriod startDate="2011-01-01" endDate="2024-12-31" isDataValidForEntirePeriod="true"/>
    </activityDescription>
    <flowData>
      <!-- OUTPUT: Treated water in m³ (reference product) -->
      <intermediateExchange id="out-1" unitId="unit-m3" amount="1.0" intermediateExchangeId="cooling-water-uuid" productionVolumeAmount="10000.0">
        <name xml:lang="en">cooling water</name>
        <unitName xml:lang="en">m3</unitName>
        <outputGroup>0</outputGroup>
