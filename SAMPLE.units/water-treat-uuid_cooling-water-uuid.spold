      <!-- INPUT: Raw water in liters (different volume unit) -->
      <intermediateExchange id="in-1" unitId="unit-l" amount="1100.0" intermediateExchangeId="raw-water-uuid" activityLinkId="water-extract-uuid">
        <name xml:lang="en">raw water</name>
        <unitName xml:lang="en">l</unitName>
        <inputGroup>5</inputGroup>
      </intermediateExchange>

      <!-- INPUT: Coagulant in ml (smallest volume unit) -->
      <intermediateExchange id="in-2" unitId="unit-ml" amount="50.0" intermediateExchangeId="coagulant-uuid" activityLinkId="chemical-prod-uuid">
        <name xml:lang="en">aluminum sulfate</name>
        <unitName xml:lang="en">ml</unitName>
        <inputGroup>5</inputGroup>
      </intermediateExchange>

      <!-- INPUT: Pipe length in km (length unit) -->
      <intermediateExchange id="in-3" unitId="unit-km" amount="0.001" intermediateExchangeId="pipe-uuid" activityLinkId="pipe-prod-uuid">
        <name xml:lang="en">distribution pipe</name>
        <unitName xml:lang="en">km</unitName>
        <inputGroup>5</inputGroup>
      </intermediateExchange>

