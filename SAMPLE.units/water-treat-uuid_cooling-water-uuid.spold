      <elementaryExchange id="elem-1" unitId="unit-m3" amount="1.05" elementaryExchangeId="freshwater-uuid">
        <name xml:lang="en">Water, fresh</name>
        <unitName xml:lang="en">m3</unitName>
        <compartment>
          <compartment xml:lang="en">natural resource</compartment>
          <subcompartment xml:lang="en">in water</subcompartment>
        </compartment>
        <inputGroup>4</inputGroup>
      </elementaryExchange>

      <!-- BIOSPHERE OUTPUT: Sludge in dm³ (deciliters, medium volume unit) -->
      <elementaryExchange id="elem-2" unitId="unit-dm3" amount="5.0" elementaryExchangeId="treatment-sludge-uuid">
